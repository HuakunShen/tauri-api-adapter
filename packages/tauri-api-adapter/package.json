{
  "name": "tauri-api-adapter",
  "type": "module",
  "version": "0.0.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/HuakunShen/tauri-api-adapter.git"
  },
  "main": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./worker": {
      "import": "./src/worker.ts"
    },
    "./native": {
      "import": "./src/native.ts"
    },
    "./iframe": {
      "import": "./src/iframe.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "rollup -c --watch",
    "build:docs": "cp ../../README.md ./README.md && typedoc src/index.ts",
    "build": "rimraf dist && rollup -c && pnpm build:docs",
    "test": "bun test --coverage"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bun": "latest",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.25.13"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@huakunshen/comlink": "^4.4.1",
    "@tauri-apps/api": "2.0.0-beta.13",
    "@tauri-apps/plugin-dialog": "2.0.0-beta.5",
    "@tauri-apps/plugin-fs": "2.0.0-beta.5",
    "@tauri-apps/plugin-http": "2.0.0-beta.6",
    "@tauri-apps/plugin-log": "2.0.0-beta.7",
    "@tauri-apps/plugin-notification": "2.0.0-beta.5",
    "@tauri-apps/plugin-os": "2.0.0-beta.5",
    "@tauri-apps/plugin-shell": "2.0.0-beta.6",
    "@tauri-apps/plugin-upload": "2.0.0-beta.7",
    "tauri-plugin-clipboard-api": "^2.0.2",
    "tauri-plugin-network-api": "^2.0.2",
    "tauri-plugin-shellx-api": "^2.0.7",
    "tauri-plugin-system-info-api": "^2.0.1",
    "zod": "^3.23.8"
  },
  "files": [
    "src",
    "dist"
  ]
}