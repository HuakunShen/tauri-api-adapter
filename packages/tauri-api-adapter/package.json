{
  "name": "tauri-api-adapter",
  "type": "module",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/HuakunTech/tauri-api-adapter.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./shell": {
      "types": "./dist/api/shell.d.ts",
      "import": "./dist/api/shell.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "rollup -c --watch --bundleConfigAsCjs",
    "build:docs": "typedoc src/index.ts",
    "build": "rollup -c && tsc-alias"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bun": "latest",
    "rollup": "^4.18.0",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.25.13"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@huakunshen/comlink": "^4.4.1",
    "@tauri-apps/api": "2.0.0-beta.13",
    "@tauri-apps/plugin-dialog": "2.0.0-beta.5",
    "@tauri-apps/plugin-fs": "2.0.0-beta.5",
    "@tauri-apps/plugin-http": "2.0.0-beta.6",
    "@tauri-apps/plugin-notification": "2.0.0-beta.5",
    "@tauri-apps/plugin-os": "2.0.0-beta.5",
    "@tauri-apps/plugin-shell": "2.0.0-beta.6",
    "tauri-plugin-clipboard-api": "^2.0.2",
    "tauri-plugin-shellx-api": "^2.0.7",
    "zod": "^3.23.8"
  },
  "files": [
    "src",
    "dist"
  ]
}