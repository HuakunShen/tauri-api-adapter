{
  "name": "tauri-api-adapter",
  "version": "0.3.5",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "require": "./dist/worker.cjs",
      "types": "./dist/worker.d.ts"
    },
    "./iframe": {
      "import": "./dist/iframe.js",
      "require": "./dist/iframe.cjs",
      "types": "./dist/iframe.d.ts"
    },
    "./server": {
      "import": "./dist/api/server/index.js",
      "require": "./dist/api/server/index.cjs",
      "types": "./dist/api/server/index.d.ts"
    },
    "./client": {
      "import": "./dist/api/client/index.js",
      "require": "./dist/api/client/index.cjs",
      "types": "./dist/api/client/index.d.ts"
    },
    "./permissions": {
      "import": "./dist/permissions/index.js",
      "require": "./dist/permissions/index.cjs",
      "types": "./dist/permissions/index.d.ts"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typedoc": "^0.26.6"
  },
  "dependencies": {
    "@huakunshen/comlink": "^4.4.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tauri-apps/api": "^2.0.0-rc.4",
    "@tauri-apps/plugin-dialog": "~2.0.0-rc.0",
    "@tauri-apps/plugin-fs": "~2.0.0-rc.0",
    "@tauri-apps/plugin-http": "~2.0.0-rc.1",
    "@tauri-apps/plugin-log": "~2.0.0-rc.0",
    "@tauri-apps/plugin-notification": "~2.0.0-rc.0",
    "@tauri-apps/plugin-os": "~2.0.0-rc.0",
    "@tauri-apps/plugin-shell": "~2.0.0-rc.0",
    "@tauri-apps/plugin-upload": "~2.0.0-rc.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.10",
    "tauri-plugin-clipboard-api": "^2.1.7",
    "tauri-plugin-network-api": "^2.0.3",
    "tauri-plugin-shellx-api": "^2.0.9",
    "tauri-plugin-system-info-api": "^2.0.4",
    "typescript": "^5.0.0",
    "valibot": "^0.40.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "dev": "rollup -c --watch",
    "build:docs": "shx cp ../../README.md ./README.md && typedoc",
    "build": "rimraf dist && rollup -c",
    "postbuild": "bun test --coverage && pnpm build:docs",
    "test": "bun test --coverage"
  },
  "files": [
    "dist"
  ]
}
