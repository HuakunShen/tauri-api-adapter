{
  "name": "tauri-api-adapter",
  "type": "module",
  "version": "0.0.9",
  "repository": {
    "type": "git",
    "url": "https://github.com/HuakunShen/tauri-api-adapter.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "require": "./dist/worker.cjs",
      "types": "./dist/worker.d.ts"
    },
    "./native": {
      "import": "./dist/native.js",
      "require": "./dist/native.cjs",
      "types": "./dist/native.d.ts"
    },
    "./iframe": {
      "import": "./dist/iframe.js",
      "require": "./dist/iframe.cjs",
      "types": "./dist/iframe.d.ts"
    },
    "./permissions": {
      "import": "./dist/permissions.js",
      "require": "./dist/permissions.cjs",
      "types": "./dist/permissions.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "rollup -c --watch",
    "build:docs": "shx cp ../../README.md ./README.md && typedoc src/index.ts",
    "build": "rimraf dist && rollup -c && pnpm build:docs",
    "test": "bun test --coverage"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/bun": "latest",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.10",
    "typedoc": "^0.25.13"
  },
  "peerDependencies": {
    "@huakunshen/comlink": "^4.4.1",
    "@tauri-apps/api": "~2.0.0-beta.15",
    "@tauri-apps/plugin-dialog": "~2.0.0-beta.7",
    "@tauri-apps/plugin-fs": "~2.0.0-beta.7",
    "@tauri-apps/plugin-http": "~2.0.0-beta.8",
    "@tauri-apps/plugin-log": "~2.0.0-beta.8",
    "@tauri-apps/plugin-notification": "~2.0.0-beta.7",
    "@tauri-apps/plugin-os": "~2.0.0-beta.7",
    "@tauri-apps/plugin-shell": "~2.0.0-beta.8",
    "@tauri-apps/plugin-upload": "~2.0.0-beta.8",
    "tauri-plugin-clipboard-api": "^2.1.6",
    "tauri-plugin-network-api": "^2.0.3-beta.1",
    "tauri-plugin-shellx-api": "^2.0.8",
    "tauri-plugin-system-info-api": "^2.0.2",
    "typescript": "^5.0.0",
    "valibot": "^0.35.0"
  },
  "files": [
    "src",
    "dist"
  ]
}