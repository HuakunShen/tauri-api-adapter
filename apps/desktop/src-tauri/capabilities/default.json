{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shellx:allow-open",
    "shellx:allow-execute",
    "shellx:allow-spawn",
    "shellx:allow-kill",
    "shellx:allow-fix-path-env",
    "clipboard:read-all",
    "clipboard:write-all"
  ]
}